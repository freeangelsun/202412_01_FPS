<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fps.acc.mapper.member.AccMemberMapper">
    <select id="selectAllMembers" resultType="fps.acc.entity.member.AccMember">
        SELECT
            id,
            name,
            description,
            created_at,
            updated_at
        FROM acc_member;
    </select>
</mapper>
